{
  "custom": {},
  "propConfig": {
    "custom.projectsFolder": {
      "binding": {
        "config": {
          "path": "this.custom.installationFolder"
        },
        "transforms": [
          {
            "code": "\tfrom com.inductiveautomation.ignition.gateway import IgnitionGateway\n\timport os\n\t\n\tdataDir \u003d IgnitionGateway.get().getSystemManager().getDataDir()\n\treturn os.path.join(dataDir.getAbsolutePath(), \"projects\")",
            "type": "script"
          }
        ],
        "type": "property"
      }
    },
    "props.auth": {
      "access": "PRIVATE",
      "persistent": false
    },
    "props.device.accelerometer": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.device.identifier": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.device.timezone": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.device.type": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.device.userAgent": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.gateway": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.geolocation.data": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.geolocation.permissionGranted": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.host": {
      "access": "SYSTEM",
      "persistent": false
    },
    "props.id": {
      "access": "SYSTEM",
      "persistent": false
    }
